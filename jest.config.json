{
  "preset": "ts-jest",
  "coverageDirectory": "coverage",
  "collectCoverageFrom": ["<rootDir>/src/**/*.ts"],
  "coveragePathIgnorePatterns": [
    "./constants.ts",
    "./main.ts",
    "index.ts",
    "executeCommand.ts"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80
    }
  }
}
